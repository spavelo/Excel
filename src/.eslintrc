{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser":  true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "indent": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "space-before-function-paren": "off",
    "no-debugger": "off",
    "no-unused-vars": "off"

  },
  "extends": ["eslint:recommended", "google"]
}